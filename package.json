{
  "name": "products114",
  "version": "0.0.1",
  "private": true,
  "description": "Products",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap/ux-specification": "UI5-1.78",
    "@sap/ux-ui5-fe-mockserver-middleware": "1",
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^2.14.1",
    "nwabap-ui5uploader": "^1.2.0"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-client=800&sap-ui-xx-viewCache=false#comprproducts114-tile\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-client=800&sap-ui-xx-viewCache=false#comprproducts114-tile\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "fiori verify",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-client=800&sap-ui-xx-viewCache=false\"",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-client=800&sap-ui-xx-viewCache=false#comprproducts114-tile\"",
    "start-variants-management": "fiori run --open \"preview.html?&sap-client=800&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "upload": "npx nwabap upload"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling",
      "@sap/ux-ui5-fe-mockserver-middleware"
    ]
  },
  "sapux": true,
  "sapuxLayer": "CUSTOMER_BASE"
}
